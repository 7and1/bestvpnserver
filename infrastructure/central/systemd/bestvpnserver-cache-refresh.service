[Unit]
Description=BestVPNServer cache refresh
After=network-online.target
Wants=network-online.target

[Service]
Type=oneshot
WorkingDirectory=/opt/docker-projects/bestvpnserver
EnvironmentFile=/opt/docker-projects/bestvpnserver/.env
ExecStart=/usr/bin/env bash /opt/docker-projects/bestvpnserver/infrastructure/central/scripts/run-cache-refresh.sh
TimeoutStartSec=600
