{
  "name": "@bestvpnserver/database",
  "version": "0.0.0",
  "private": true,
  "types": "src/index.ts",
  "exports": {
    ".": "./src/index.ts"
  },
  "scripts": {
    "typecheck": "tsc -p tsconfig.json",
    "db:generate": "drizzle-kit generate --config ../../drizzle.config.ts",
    "db:push": "drizzle-kit push --config ../../drizzle.config.ts",
    "db:migrate": "drizzle-kit migrate --config ../../drizzle.config.ts",
    "db:seed": "tsx src/seed.ts"
  },
  "dependencies": {
    "drizzle-orm": "^0.34.1",
    "pg": "^8.11.5"
  },
  "devDependencies": {
    "@types/node": "^20.19.28",
    "@types/pg": "^8.11.10",
    "drizzle-kit": "^0.25.0",
    "dotenv": "^16.4.5",
    "tsx": "^4.16.2",
    "typescript": "^5.9.3"
  }
}
